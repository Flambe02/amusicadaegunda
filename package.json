{
  "name": "musica-da-segunda",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node scripts/generate-stubs.cjs",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "preview": "vite preview",
    "deploy": "npm run build && node deploy-docs.js",
    "cap:init": "cap init \"Música da Segunda\" com.tonorg.musicadasegunda || true",
    "cap:add:android": "cap add android || true",
    "cap:add:ios": "cap add ios || true",
    "cap:copy": "cap copy",
    "cap:sync": "cap sync",
    "cap:open:android": "cap open android",
    "cap:open:ios": "cap open ios",
    "icons:ios": "cordova-res ios --skip-config --copy",
    "icons:android": "node scripts/generate-android-assets.js",
    "splash:android": "echo 'Splash screen géré par le script icons:android'",
    "assets:android": "npm run icons:android",
    "ci:android": "npm ci && npm run build && npx cap sync android",
    "sitemap": "node scripts/generate-sitemap.cjs",
    "predeploy": "npm run sitemap",
    "prepare": "husky install",
    "test:admin": "node scripts/test-admin-crud.js",
    "test:push": "node scripts/test-push-upsert.js",
    "test:all": "node scripts/test-admin-crud.js && node scripts/test-push-upsert.js",
    "postbuild": "node scripts/generate-stubs.cjs && node scripts/generate-sitemap.cjs",
    "export:songs": "node scripts/export-songs-from-supabase.cjs",
    "build:seo": "npm run export:songs && npm run build",
    "qa:jsonld": "node scripts/check-jsonld-prod.js",
    "qa:sitemap": "node scripts/check-sitemap-prod.js",
    "qa:prod": "npm run qa:jsonld && npm run qa:sitemap"
  },
  "dependencies": {
    "@capacitor/core": "^7.4.3",
    "@capacitor/splash-screen": "^7.0.2",
    "@hookform/resolvers": "^4.1.2",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "embla-carousel-react": "^8.5.2",
    "framer-motion": "^12.4.7",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.475.0",
    "next-themes": "^0.4.4",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.54.2",
    "react-resizable-panels": "^2.1.7",
    "react-router-dom": "^7.2.0",
    "recharts": "^2.15.1",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "web-vitals": "^5.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@eslint/js": "^9.19.0",
    "@flydotio/dockerfile": "^0.7.8",
    "@supabase/supabase-js": "^2.76.1",
    "@types/node": "^22.13.5",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "cordova-res": "^0.15.4",
    "cross-env": "^10.0.0",
    "cssnano": "^7.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.2",
    "globals": "^15.14.0",
    "he": "^1.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "postcss": "^8.5.3",
    "sitemap": "^8.0.1",
    "tailwindcss": "^3.4.17",
    "terser": "^5.43.1",
    "vite": "^6.1.0",
    "web-push": "^3.6.7"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "cross-env NODE_ENV=production eslint --max-warnings=0 --fix"
  }
}